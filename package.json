    "rules": {
      "string-quotes": "single"
    }
  },
  "scripts": {
    "eslint": "eslint ./src",
    "stylelint": "stylelint \"src/styles/*.less\" --syntax less",
    "lint": "npm run eslint",
    "validate": "npm ls",
    "test": "mocha --compilers js:babel-register --recursive",
    "test:watch": "mocha --compilers js:babel-register --reporter min --watch",
    "clean": "node bin/run clean",
    "copy": "node bin/run copy",
    "build": "node bin/run build",
    "build:debug": "node bin/run build --debug",
    "publish": "node bin/run publish",
    "publish:debug": "node bin/run publish --debug",
    "start": "node bin/run start && open http://127.0.0.1:8080/ && webpack-dev-server --hot --config config/webpack.config.dev.js"
  },
